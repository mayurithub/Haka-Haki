<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mayurit.hakahaki">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Notice" />
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoryDetail" />
        <activity android:name=".ActivityPostDetail" />
        <activity android:name=".PostDetail" />
        <activity android:name=".AudioActivity" />
        <activity android:name=".AudioDetail" />
        <activity android:name=".VideoDetail" />
        <activity android:name=".ProjectDetail" />
        <activity android:name=".ActivityProjectDetail" />
        <activity android:name=".NEEFEJDetail" />
        <activity android:name=".ActivityPostTypeDetail" />
        <activity android:name=".Model.Videoplay" />
        <activity android:name=".ActivityPostTypeList" />
        <activity android:name=".ActivityAudio"></activity>
        <activity android:name=".ActivityMemberList"></activity>
    </application>

</manifest>